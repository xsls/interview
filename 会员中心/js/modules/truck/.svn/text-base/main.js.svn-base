define(["jquery","modules/truck/list","modules/truck/detail"],
    function ($,list,detail) {
        return {
            init: function () {
                if ($(".mycarlist").length > 0 ) {
                    list.init();
                }
                if ($(".mycardetail").length > 0 ) {
                	detail.init();
                }
                if ($(".mycaredit").length > 0 ) {
                	detail.init();
                }
            }
        };

    });